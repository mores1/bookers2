<div>
    <!--%= render 'list', user: current_user %>-->
    <%= render 'list', user: @user %>
    <%= render 'books/list', book: @book %>
</div>

<!--get_profile_image-->
<div>
    <h2>Users</h2>
    <h4>image</h4>
    <h4>name</h4>
    <h4>(Showbotanran)</h4>
    <% @users.each do |user| %>
        <div>
            <%= image_tag user.get_profile_image(100,100) %>
            <%= user.name %>
            <%= link_to "Show", user_path(user.id) %>
        </div>
    <% end %>
</div>